generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model reserva {
  id       Int     @id @default(autoincrement())
  nombre   String
  dia      String
  hora     String
  telefono String
  email    String
  mas_info String?
  personas Int?
}
